<h1>To Do List</h1>
<% @lists.each do |list| %>
  <h2>Item: <%= list.item %></h2>
  <h3>Completed: <%= list.completed ? "Yes" : "No"%></h3>
  <h4>By: <%= list.author %></h4>
  <%= link_to 'New Item', new_list_path %>
  <%= link_to 'View', list_path(list) %>
  <%= link_to 'Edit', edit_list_path(list) %>
  <%= link_to 'Delete', list, method: :delete %>
  <br></br>
  <% end %>